{
    "name": "Bagelbot Dashboard Dark Mode",
    "version": "0.0.1",
    "description": "Bagelbot Dashboard dark mode injecter",
    "minimum_chrome_version": "96",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "permissions": [
      "tabs",
      "storage", "activeTab", "scripting"
    ]
  }
